server {

	listen 443 ssl default_server;
	listen [::]:443 ssl default_server;

        ssl on;
        ssl_certificate /etc/letsencrypt/live/spatial.gsat.technology/cert.pem;
        ssl_certificate_key /etc/letsencrypt/live/spatial.gsat.technology/privkey.pem;

	root /var/www/html;

	server_name {SERVER_NAME};

	location / {
		# First attempt to serve request as file, then
		# as directory, then fall back to displaying a 404.
		try_files $uri $uri/ =404;
	}
}
